{
  "version": 2,
  "routes": [
    {
      "src": "/s/([^/]+)",
      "dest": "/api/redirect?code=$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
